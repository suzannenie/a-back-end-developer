{
  "name": "A Back-End Developer",
  "description" : "What do you call a software engineer doing squats?",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "dogsquat.png"
  },
  "icons": {
    "16": "dogsquat.png",
    "48": "dogsquat.png",
    "128": "dogsquat.png"
  },
  "permissions": [
      "tabs",
      "background",
      "alarms",
      "storage",
      "https://*/",
      "http://*/"
    ],
  "content_scripts": [
  {
    "matches": [
      "<all_urls>"
    ],
    "js": ["content.js"]
  }
  ],
  "background": {
        "scripts": [
          "background.js"
        ],
        "persistent": false
    }
}